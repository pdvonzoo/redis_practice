      "@types/yup": "^0.26.14",
      "gql2ts": "^1.10.1",
      "jest": "^24.8.0",
      "nodemon": "^1.19.1",
      "ts-jest": "^24.0.2",
      "ts-node": "^8.2.0",
      "tslint": "^5.17.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.5.1"
   },
   "dependencies": {
      "@gql2ts/from-schema": "^1.10.1",
      "@types/express": "^4.17.0",
      "@types/sparkpost": "^2.1.4",
      "@types/uuid": "^3.4.4",
      "bcryptjs": "^2.4.3",
      "dotenv": "^8.0.0",
      "express": "^4.17.1",
      "graphql": "^14.3.1",
      "graphql-import": "^0.7.1",
      "graphql-request": "^1.8.2",
      "graphql-tools": "^4.0.4",
      "graphql-yoga": "^1.17.4",
