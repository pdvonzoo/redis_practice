{
   "name": "graphql-ts-server",
   "version": "0.0.1",
   "description": "Awesome project developed with TypeORM.",
   "devDependencies": {
      "@types/bcryptjs": "^2.4.2",
      "@types/ioredis": "^4.0.11",
      "@types/jest": "^24.0.13",
      "@types/node": "^12.0.4",
      "@types/yup": "^0.26.14",
      "gql2ts": "^1.10.1",
      "jest": "^24.8.0",
      "nodemon": "^1.19.1",
      "ts-jest": "^24.0.2",
      "ts-node": "^8.2.0",
      "tslint": "^5.17.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.5.1"
   },
   "dependencies": {
      "@types/uuid": "^3.4.4",
      "bcryptjs": "^2.4.3",
      "graphql": "^14.3.1",
      "graphql-import": "^0.7.1",
      "graphql-request": "^1.8.2",
      "graphql-tools": "^4.0.4",
      "graphql-yoga": "^1.17.4",
      "ioredis": "^4.9.5",
      "pg": "^7.11.0",
      "reflect-metadata": "^0.1.13",
      "typeorm": "0.2.17",
      "uuid": "^3.3.2",
      "yup": "^0.27.0"
   },
   "scripts": {
      "start": "NODE_ENV=developement nodemon --exec ts-node --files src/index.ts",
      "test": "NODE_ENV=test jest --forceExit --detectOpenHandles",
      "gen-schema-types": "gql2ts src/schema.graphql -o src/types/schema.d.ts"
   },
   "jest": {
      "transform": {
         "^.+\\.tsx?$": "ts-jest"
      },
      "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
      "moduleFileExtensions": [
         "ts",
         "tsx",
         "js",
         "jsx",
         "json",
         "node"
      ]
   }
}
